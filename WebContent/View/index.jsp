<main role="main">

 <table data-toggle="table" id="table"
  data-toggle="table"
  data-height="600"
  data-pagination="true"
  data-pagination-v-align="both">

  <!--Table head-->
  <thead>
    <tr>
      <th>Id</th>
      <th>Note</th>
      <th>Timestamp</th>
      <th>Nonce</th>
      <th>PrevHash</th>
      <th>Hash</th>
      
    </tr>
  </thead>
  <!--Table head-->

  <!--Table body-->
  <tbody>
  	<c:forEach items="${blockChain}" var="block">
	    <tr class="table-info">
	      <th scope="row">${block.getIndex()}</th>
	      <td>${block.getNote()}</td>
	      <td>${block.getTimestamp()}</td>
	      <td>${block.getNonce()}</td>
	      <td>${block.getPrev_hash()}</td>
	      <td>${block.getHash()}</td>
	      
	    </tr>
	</c:forEach>
    
  </tbody>
  <!--Table body-->
</table>
<!--Table-->
  
</main>

